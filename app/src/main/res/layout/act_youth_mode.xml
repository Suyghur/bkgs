<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include layout="@layout/top_titlebar_layout" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <WebView
            android:id="@+id/youthWv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/startYouthTv" />

        <TextView
            android:id="@+id/startYouthTv"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_42"
            android:layout_alignParentBottom="true"
            android:layout_marginHorizontal="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginBottom="@dimen/dp_100"
            android:background="@drawable/selector_youth_close_bg"
            android:gravity="center"
            android:text="开启模式"
            android:textColor="@drawable/selector_color_youth_textcolor"
            android:textSize="@dimen/dp_16" />

    </RelativeLayout>
</LinearLayout>